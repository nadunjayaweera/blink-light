const int led1 = 1;
const int led2 = 2;
const int led3 = 3;
const int led4 = 4;
void setup() {
  // put your setup code here, to run once:
pinMode(led1,OUTPUT);
pinMode(led2, OUTPUT);
pinMode(led3,OUTPUT);
pinMode(led4,OUTPUT);
}

void loop() {
  // put your main code here, to run repeatedly:
  //patern 1
  for(int i=0; i<4; i++){
digitalWrite(led1,HIGH);
digitalWrite(led2,HIGH);
